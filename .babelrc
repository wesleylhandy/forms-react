{
    "plugins": [
      "react-hot-loader/babel",
      ["@babel/plugin-transform-runtime",
        {
          "regenerator": true
        }
      ],
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ],
      "transform-inline-environment-variables"
    ],
    "presets": [
      [
        "@babel/preset-env", 
        { 
          "useBuiltIns": "entry", 
          "corejs": 3, 
          "targets": { 
            "node": "current" 
          } 
        }
      ], 
      "@babel/preset-react", 
      [
        "@emotion/babel-preset-css-prop",
        {
          "autoLabel": true,
          "labelFormat": "[local]"
        }
      ]
    ]
  }